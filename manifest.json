{
  "manifest_version": 2,

  "name": "Infogalactic Transporter",
  "description": "This extension allows the user to move between Wiki and IG.",
  "icons": { "16": "IW-16.png",
           "48": "IW-48.png",
          "128": "IW-128.png" },
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_title": "Get me outta here!"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.3.1.min.js","redirect.js"]
  }
]
}
